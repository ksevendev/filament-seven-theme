{
    "name": "kseven/filament-seven-theme",
    "description": "A modern, responsive, and customizable theme for FilamentPHP, designed to elevate your admin panel with a sleek interface and seamless user experience.",
    "keywords": [
        "Resma",
        "laravel",
        "filamentphp",
        "filament-seven-theme",
        "theme",
        "filament"
    ],
    "homepage": "https://github.com/ksevendev/filament-seven-theme",
    "support": {
        "issues": "https://github.com/ksevendev/filament-seven-theme/issues",
        "source": "https://github.com/ksevendev/filament-seven-theme"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "K'Seven",
            "email": "contato@kseven.dev.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KSeven\\FilamentSevenTheme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KSeven\\FilamentSevenTheme\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "lint": "pint",
        "test": "pest",
        "test:lint": "pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                
            ],
            "aliases": {
                "FilamentSevenTheme": "KSeven\\FilamentSevenTheme\\Facades\\FilamentSevenTheme"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}